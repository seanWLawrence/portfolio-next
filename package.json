{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "0.0.0-development",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "next",
    "gatsby-image": "^1.0.55",
    "gatsby-link": "^1.6.46",
    "gatsby-plugin-babel": "^1.0.0",
    "gatsby-plugin-catch-links": "^1.0.24",
    "gatsby-plugin-fastclick": "^1.0.1",
    "gatsby-plugin-favicon": "^3.1.2",
    "gatsby-plugin-manifest": "^2.0.2-beta.4",
    "gatsby-plugin-netlify-cms": "^2.0.1",
    "gatsby-plugin-offline": "^2.0.0-beta.9",
    "gatsby-plugin-preact": "^1.0.17",
    "gatsby-plugin-react-helmet": "^3.0.0-beta.4",
    "gatsby-plugin-sharp": "^1.6.48",
    "gatsby-plugin-sitemap": "^1.2.25",
    "gatsby-remark-autolink-headers": "^1.4.19",
    "gatsby-remark-copy-linked-files": "^1.5.37",
    "gatsby-remark-images": "^1.5.67",
    "gatsby-source-filesystem": "^1.5.39",
    "gatsby-transformer-remark": "^1.7.44",
    "gatsby-transformer-sharp": "^1.6.27",
    "gatsby-transformer-yaml": "^1.5.18",
    "preact": "^8.3.0",
    "preact-compat": "^3.18.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "flow": "flow",
    "flow:stop": "flow stop",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "npm run flow:stop && npm run flow && npm test",
    "lint": "prettier-eslint --config .prettierrc.js --eslint-config-path .eslintrc.js --write  \"src/*.js\"",
    "prepare": "npm run test:ci && npm run lint",
    "build": "npm run prepare && gatsby build",
    "commit": "npm run test:ci && git-cz",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once \"npm run semantic-release\""
  },
  "devDependencies": {
    "prettier": "^1.13.7",
    "semantic-release": "^15.9.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
